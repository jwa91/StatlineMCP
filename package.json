{
  "name": "cbs-statline-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for interacting with CBS Statline OData API",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "pnpm run build && pnpm run start",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "check": "biome check --write ./src"
  },
  "keywords": [
    "mcp",
    "cbs",
    "statline",
    "odata",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.3",
    "typescript": "^5.8.3"
  }
}